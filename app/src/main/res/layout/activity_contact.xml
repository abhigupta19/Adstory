<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/bb"
    tools:context="com.yash.user.winters.contact">

    <TextView
        android:id="@+id/textView4"
        android:layout_width="283dp"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_marginStart="24dp"
        android:layout_marginLeft="10dp"
        android:layout_marginTop="10dp"
        android:fontFamily="serif"
        android:text="Contact us"
        android:textColor="#ffffff"
        android:textSize="24sp" />

    <ImageView
        android:id="@+id/imageView"
        android:layout_width="147dp"
        android:layout_height="47dp"
        android:layout_below="@+id/imageView9"
        android:layout_marginLeft="100dp"
        android:layout_marginTop="60dp"
        app:srcCompat="@android:drawable/sym_action_email" />

    <TextView
        android:id="@+id/textView5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/imageView"
        android:layout_marginLeft="152dp"
        android:layout_marginTop="0dp"
        android:text="(Email)"
        android:textColor="@color/wallet_bright_foreground_holo_dark" />

    <TextView
        android:id="@+id/textView6"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@+id/imageView10"
        android:layout_alignParentStart="true"
        android:layout_marginStart="24dp"
        android:layout_marginTop="280dp"
        android:fontFamily="serif"
        android:text="Developer"
        android:textColor="@color/cast_expanded_controller_text_color"
        android:textSize="24sp" />

    <de.hdodenhof.circleimageview.CircleImageView xmlns:app="http://schemas.android.com/apk/res-auto"
        android:id="@+id/profile_image"
        android:layout_width="126dp"
        android:layout_height="114dp"
        android:layout_marginStart="10dp"
        android:layout_marginLeft="15dp"
        android:layout_marginTop="350dp"
        android:src="@drawable/abhi"
        app:civ_border_color="#FF000000"
        app:civ_border_width="2dp" />

    <ImageView
        android:id="@+id/imageView2"
        android:layout_width="69dp"
        android:layout_height="60dp"
        android:layout_above="@+id/imageView10"
        android:layout_marginLeft="150dp"
        android:layout_marginTop="385dp"
        android:layout_marginBottom="-169dp"
        android:src="@drawable/facebookw" />

    <TextView
        android:id="@+id/textView"
        android:layout_width="230dp"
        android:layout_height="wrap_content"
        android:layout_above="@+id/imageView10"
        android:layout_marginLeft="160dp"
        android:layout_marginTop="345dp"
        android:layout_marginBottom="-97dp"
        android:fontFamily="casual"
        android:text="Abhinav gupta"
        android:textColor="@color/cast_expanded_controller_text_color"
        android:textSize="20sp" />

    <ImageView
        android:id="@+id/imageView3"
        android:layout_width="75dp"
        android:layout_height="68dp"
        android:layout_marginLeft="220dp"
        android:layout_marginTop="380dp"
        android:src="@drawable/gitmin" />

    <TextView
        android:id="@+id/textView17"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignStart="@+id/textView4"
        android:layout_alignParentTop="true"
        android:layout_marginStart="-10dp"
        android:layout_marginTop="140dp"
        android:text="How to share/submit /reviews  "
        android:textColor="#ffffff"
        android:textSize="20sp" />

    <ImageView
        android:id="@+id/imageView4"
        android:layout_width="49dp"
        android:layout_height="28dp"
        android:layout_below="@+id/textView17"
        android:layout_alignStart="@+id/textView"
        android:layout_marginStart="-10dp"
        android:layout_marginTop="17dp"
        app:srcCompat="@drawable/llnew" />

</RelativeLayout>